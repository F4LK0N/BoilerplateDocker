#!/bin/sh

if [[ "${1}" == "[CLEAR]" ]]; then
    rm -rf /run/container/setup-fs-error;
else
    echo -n "!";
    
    padding="______________________________________________________________________";
    printf "%s%s" "$setupFsPath" "${padding:${#setupFsPath}}";

    echo "[ERROR] ${setupFsError}";

    mkdir -p 777 /run/container/setup-fs-error;
fi;
